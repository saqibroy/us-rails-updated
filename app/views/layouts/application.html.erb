<body class="home-page">
<div class="wrapper">
  <%= render 'layouts/header' %>
  <!-- ******CONTENT****** -->
  <div class="content container">
    <%= yield %>
  </div><!--//content-->
</div><!--//cols-wrapper-->
<%= render 'layouts/footer' %>
</body>
</html>
